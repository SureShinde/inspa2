<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="search.result">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Magento_CatalogSearch::result.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="product_list_toolbar">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Magento_Catalog::product/list/toolbar.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
